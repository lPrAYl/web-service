{
  "name": "@web-service/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' \"!**/{node_modules,dist}/**/*\"",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache \"src/**/*.scss\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.7.0",
    "@trpc/react-query": "^11.7.0",
    "@types/sass": "^1.45.0",
    "@web-service/backend": "workspace:*",
    "include-media": "^2.0.0",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "reset-css": "^5.0.2",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "ts-node": "^10.9.2",
    "vite": "^7.1.7"
  }
}
